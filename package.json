{
  "name": "erela.js",
  "version": "3.0.0",
  "description": "An easy-to-use Lavalink v4 client for NodeJS with advanced features.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [  "dist" ],
  "scripts": {
    "build": "tsc",
    "types": "rtb --dist dist",
    "lint": "eslint --ext .ts ./src",
    "docs": "typedoc --json ./docs.json --mode file --excludeProtected --excludePrivate --excludeExternals src/structures",
    "publish:stable": "yarn build && yarn types && yarn publish --access=public",
    "publish:beta": "yarn build && yarn types && yarn publish --tag beta --access=public",
    "ci": "run-s lint build types"
  },
  "keywords": [
    "lavalink",
    "lavalink-v4",
    "discord",
    "music",
    "bot",
    "discord.js",
    "eris",
    "audio",
    "filters"
  ],
  "author": "Forker: ryxu-xo (https://github.com/ryxu-xo) | MenuDocs (https://github.com/MenuDocs)",
  "contributors": [
    {
      "name": "ryxu-xo",
      "url": "https://github.com/ryxu-xo",
      "role": "v3.0 Modernization & Lavalink v4 Support"
    },
    {
      "name": "Solaris9",
      "url": "https://mhargreaves.site"
    },
    {
      "name": "Anish-Shobith",
      "url": "https://anishshobithps.tech"
    },
    {
      "name": "melike2d",
      "email": "hi@2d.gay",
      "url": "https://2d.gay"
    },
    {
      "name": "ayntee"
    }
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/ryxu-xo/erela.js.git"
  },
  "bugs": {
    "url": "https://github.com/ryxu-xo/erela.js/issues"
  },
  "devDependencies": {
    "@favware/rollup-type-bundler": "^3.3.0",
    "@types/node": "^20.11.0",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "npm-run-all": "^4.1.5",
    "typedoc": "^0.26.0",
    "typedoc-plugin-no-inherit": "^1.6.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@discordjs/collection": "^2.1.0",
    "tslib": "^2.6.2",
    "undici": "^6.6.0",
    "ws": "^8.16.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "eslintConfig": {
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "object-curly-spacing": [
        "error",
        "always"
      ]
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended"
    ]
  },
  "homepage": "https://github.com/ryxu-xo/erela.js#readme"
}
